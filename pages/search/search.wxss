/* pages/search/search.wxss */

.phone_input {
  margin-top: 40rpx;
  border-bottom: 1rpx solid #ccc;
}

.phone_input button {
  width: 140rpx;
  height: 60rpx;
  border-radius: 3rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  background: #ee8e86;
  color: white;
}

.phone_input text {
  font-size: 32rpx;
  margin-top: 15rpx;
  font-weight: normal;
}

.phone_input  input {
  width: 300rpx;
  height: 58rpx;
  border: 1px solid #ccc;
  margin-left: 15rpx;
  font-size: 28rpx;
  padding-left: 10rpx;
}

.box {
  display: flex;
  flex-direction: row;
  margin-bottom: 40rpx;
}
.box .text{
  height: 60rpx;
  line-height: 60rpx;
}
.table {
  width: 710rpx;
  border: 1rpx solid #ee8e86;
   margin: 0 auto;
  margin-top: 52rpx;
 
}

.tr_top {
  width: 100%;
  height: 54rpx;
  background: #ee8e86;
}

.th {
  width: 25%;
  display: inline-block;
  color: white;
  font-size: 26rpx;
  text-align: center;
}

.td {
  width: 25%;
  display: inline-block;
  font-size: 26rpx;
  text-align: center;
}
.tr{
  height: 66rpx;
  line-height: 66rpx;
  border-bottom: 1rpx solid #e5e5e5;
 
}